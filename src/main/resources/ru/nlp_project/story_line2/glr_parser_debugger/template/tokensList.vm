					<table>
						<tr><th>Value</th><th>Lemm</th><th>POS</th><th>Grammemes</th><th>exact</th></tr>
					#foreach ($l in $t.getLexemesListCopy(true))
						<tr><td>$l.value</td><td>$l.lemm</td><td>$l.POS</td><td>$l.grammemes</td><td>$l.exactMatch</td></tr>
					#end	
					</table>
					#end					
					</div>
					</span>
				#end
				</div> <!--<div class="data">-->
